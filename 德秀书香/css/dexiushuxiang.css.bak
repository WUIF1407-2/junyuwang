.content{
height:900px;width:1076px;margin:0 auto;
}
body{
background:#f3f3f3;
}
ul,li{
list-style:none;margin:0px;padding:0px;
}
a{
text-decoration:none;
}

.content-left{
width:254px;height:380px;background:#fff;float:left;margin-top:20px;
}
.content-left a{
color:#888989;line-height:33px;display:inline-block;width:234;height:33px;padding-left:20px;
}
.content-left a:hover{
color:#fff;background:#009494;
}

.content-left li{
border-bottom:1px solid #ccc;height:33px;
}
.content-right{
width:800px;height:880px;background:#fff;float:right;margin-top:20px;
}